{
    "settings_popup": {
        "type": "dock",
        "content": {
            "type": "array",
            "orientation": "vertical",
            "h_alignment": "left",
            "content": [
                {
                    "type": "group",
                    "header": "Settings",
                    "content": {
                        "type": "layout",
                        "key": "setting_grid"
                    }
                },
                {
                    "type": "group",
                    "header": "Events",
                    "content": {
                        "type": "layout",
                        "key": "events_grid"
                    }
                }
            ]
        }
    },
    "tracker_default": {
        "type": "container",
        "background": "#00000000",
        "content": {
            "type": "dock",
            "dropshadow": true,
            "content": [
                {
                    "type": "dock",
                    "dock": "left",
                    "v_alignment": "stretch",
                    "content": [
                        {
                            "type": "group",
                            "header": "Forms",
                            "dock": "top",
                            "content": {
                                "type": "layout",
                                "key": "forms"
                            }
                        },
                        {
                            "type": "group",
                            "header": "Transturtles",
                            "dock": "top",
                            "content": {
                                "type": "layout",
                                "key": "turtles"
                            }
                        },
                        {
                            "type": "group",
                            "header": "Misc Progression",
                            "dock": "top",
                            "content": {
                                "type": "layout",
                                "key": "misc"
                            }
                        },
                        {
                            "type": "group",
                            "header": "Useful Items",
                            "dock": "top",
                            "content": {
                                "type": "layout",
                                "key": "useful"
                            }
                        },
                        {
                            "type": "group",
                            "header": "Tracker Settings",
                            "dock": "top",
                            "content": {
                                "type": "layout",
                                "key": "quicksettings"
                            }
                        }
                    ]
                },
                {
                    "type": "dock",
                    "content": {
                        "type": "layout",
                        "key": "tabs"
                    }
                }
            ]
        }
    }
}