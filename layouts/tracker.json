{
  "settings_popup": {
    "type": "dock",
    "content": {
      "type": "array",
      "orientation": "vertical",
      "h_alignment": "left",
      "content": [
        {
          "type": "group",
          "header": "Settings",
          "dock": "Settings",
          "content": {
            "type": "layout",
            "key": "setting_grid"
          }
        }
      ]
    }
  },
    "tracker_default": {
        "type": "container",
        "background": "#00000000",
        "content": {
            "type": "dock",
            "dropshadow": true,
            "content": [
                {
                    "type": "dock",
                    "dock": "left",
                    "v_alignment": "stretch",
                    "content": [
                        {
                            "type": "group",
